<% content_for :title, "OrgPoint | #{@company.title}" %>
<div class="row">
	<div class="large-12 column">
		<center><h1><%= @company.title %></h1></center>
		<hr />
		<% @company.users.each do |user| %>
			<%= link_to user_path(user.id) do %>
				<h3><%= user.first_name %> <%= user.last_name %></h3>
			<% end %>
		<% end %>
	</div>
</div>
